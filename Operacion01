#include <iostream>

using namespace std;

int main()
{
   int a,b,c,d,e,f;
    cout<<"Escribir primer operando: ";
    cin>>a;
    cout<<"Escribir segundo operando: ";
    cin>>b;
    c=a+b;
    d=a-b;
    e=a*b;
    f=a%b;
    cout<<"La suma es: "<<c<<endl;
    cout<<"La resta es: "<<d<<endl;
    cout<<"El producto es: "<<e<<endl;
    cout<<"el modulo es: "<<f<<endl;
    return 0;
}
